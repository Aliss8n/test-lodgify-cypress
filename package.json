{
  "name": "test-lodgify-cypress",
  "version": "1.0.0",
  "description": "Automation tests to Lodgify",
  "scripts": {
    "test": "cypress run",
    "test:headless": "cypress open",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "author": "Alisson Foga√ßa",
  "license": "ISC",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "cypress": "^14.5.1",
    "path": "^0.12.7"
  },
  "cypress-cucumber-preprocessor": {
    "filterSpecs": true,
    "omitFiltered": true,
    "stepDefinitions": [
      "cypress/e2e/[filepath]/*.js",
      "cypress/e2e/common/*.js",
      "cypress/e2e/hooks/*.js"
    ]
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-commandline": "^2.34.1",
    "better-sqlite3": "^12.2.0"
  }
}
